{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/stripe/webhook",
      "dest": "/api/stripe/webhook.js"
    },
    {
      "src": "/api/stripe/sync-plan",
      "dest": "/api/stripe/sync-plan.js"
    },
    {
      "src": "/api/health",
      "dest": "/api/health.js"
    },
    {
      "src": "/api/ready",
      "dest": "/api/ready.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/[...catchall].js"
    }
  ]
}
