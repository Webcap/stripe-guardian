{
  "name": "Wiznote",
  "main": "expo-router/entry",
  "version": "1.1.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "dev:clean-users": "node ./scripts/dev-clean-users.js",
    "dev:reset-db": "node ./scripts/dev-reset-database.js",
    "update-plan-ids": "node ./scripts/update-plan-ids.js",
    "migrate-feature-systems": "node ./scripts/run-feature-systems-migration.js",
    "android": "expo run:android",
    "dev": "APP_VARIANT=development npx expo start",
    "dev:full": "concurrently \"npm run webhook:dev\" \"npm run stripe:guardian:docker\" \"APP_VARIANT=development npx expo start\"",
    "dev:webhook": "concurrently \"npm run webhook:dev\" \"APP_VARIANT=development npx expo start\"",
    "dev:stripe": "concurrently \"npm run stripe:guardian:docker\" \"APP_VARIANT=development npx expo start\"",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "webhook:dev": "node server/webhook-server.js",
    "stripe:health-check": "node scripts/stripe-health-check.js",
    "stripe:monitor": "node scripts/webhook-monitor.js",
    "stripe:test-verification": "node scripts/test-immediate-verification.js",
    "stripe:fix-customer-id": "node scripts/fix-stripe-customer-id.js",
    "stripe:fix-user": "node scripts/fix-specific-user.js",
    "stripe:find-customer": "node scripts/find-stripe-customer.js",
    "stripe:guardian": "node scripts/auto-stripe-guardian.js",
    "stripe:guardian:docker": "docker-compose -f docker-compose.stripe-guardian.yml up -d",
    "stripe:guardian:stop": "docker-compose -f docker-compose.stripe-guardian.yml down",
    "deploy:vercel": "vercel --prod",
    "vercel": "vercel",
    "test:mobile-payments": "npx expo start --tunnel",
    "test:mobile-payment": "node scripts/test-mobile-payment-fallback.js",
    "lint": "expo lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "build:preview": "eas build --profile preview --platform all",
    "build:preview:android": "eas build --profile preview --platform android",
    "build:preview:ios": "eas build --profile preview --platform ios",
    "build:production": "eas build --profile production --platform all",
    "submit:production": "eas submit --profile production",
    "version": "npm version",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major",
    "version:show": "npm version --no-git-tag-version"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "@stripe/stripe-react-native": "0.45.0",
    "@supabase/supabase-js": "^2.53.0",
    "@types/stripe": "^8.0.416",
    "base64-js": "^1.5.1",
    "better-auth": "^1.3.4",
    "dotenv": "^17.2.1",
    "drizzle-orm": "^0.44.4",
    "drizzle-zod": "^0.8.2",
    "expo": "^53.0.20",
    "expo-audio": "^0.4.8",
    "expo-av": "~15.1.7",
    "expo-blur": "~14.1.5",
    "expo-constants": "~17.1.7",
    "expo-dev-client": "~5.2.4",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.4.0",
    "expo-linking": "~7.1.7",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.10",
    "expo-web-browser": "~14.2.0",
    "postgres": "^3.4.7",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5",
    "stripe": "^18.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.9.0",
    "@types/jest": "^29.5.12",
    "@types/react": "~19.0.10",
    "concurrently": "^9.2.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "jest": "^29.7.0",
    "react-test-renderer": "19.0.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3"
  },
  "private": true
}
